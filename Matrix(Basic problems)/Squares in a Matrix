#include <iostream>
using namespace std;
#include <bits/stdc++.h>
int main() {
	//code
	int t;
	cin>>t;
	while(t--)
	{
	int n, m, k;
	long int sum=0;
	cin>>m>>n;
	if(m < n)
    k = m;
    else
    k = n;
    while(k>0){
    sum+=(m-k+1)*(n-k+1);//FORMULA BASED
    k--;
	}
	cout<<sum<<endl;
  }
	return 0;
}
