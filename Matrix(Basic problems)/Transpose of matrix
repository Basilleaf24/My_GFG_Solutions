#include <iostream>
using namespace std;

int main() {
	//code
	int t;
	cin>>t;
	while(t--)
	{
	    int n;
	    cin>>n;
	    int i, j, arr[n][n];
	    for(i=0; i<n; i++)
	    {
	      for(j=0; j<n; j++)
	        cin>>arr[i][j];  
	    }
	    for(i=0; i<n; i++)
	    {
	      for(j=0; j<n; j++){
	        //arr[j][i]=arr[i][j]; 
	        cout<<arr[j][i]<<" ";
	      }
	    }
	     cout<<endl;
	}
	return 0;
}
