#include<bits/stdc++.h>
using namespace std;
bool sortacc(pair<char,int>&a,pair<char,int>&b)
{
    if(a.second==b.second)
      return (a.first<b.first);
    else
      return (a.second>b.second);
}
int main()
 {
    int t;
    cin>>t;
    string s;
    while(t--)
    {
      map<char,int>m;
      cin>>s;
      for(int i=0;i<s.size();i++)
      {
          m[s[i]]++;
      }
      vector<pair<char,int>>v(m.begin(),m.end());
      sort(v.begin(),v.end(),sortacc);
      auto it=v.begin();
      for( ;it!=v.end();it++)
      cout<<it->first;
      cout<<endl;
        
    }
	return 0;
}
