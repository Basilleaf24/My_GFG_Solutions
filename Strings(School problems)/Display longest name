#include <iostream>
using namespace std;

int main() {
	//code
	int t;
	cin>>t;
	while(t--)
	{
	    int n, i;
	    cin>>n;
	    string max="";
	    string s;
	    for(i=0; i<n; i++)
	    {
	        cin>>s;
	        if(s.length()>max.length())
	            max=s;
	    }
	    cout<<max<<endl;
	}
	return 0;
}

