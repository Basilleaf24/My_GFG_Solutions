/*TAKING 2 ONES FROM N ONES AT A TIME, PERMUTATIONS CONCEPT, NC2=(N*(N-1))/2    */

long binarySubstring(int n, string a){
    
    // Your code here
    int i, count=0;
    for(i=0; i<a.length(); i++)
    {
        if(a.at(i)=='1')               //put 1 in single quote
            count++;
    }
    return (count*(count-1))/2;
}
