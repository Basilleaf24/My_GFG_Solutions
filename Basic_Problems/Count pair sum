#include <iostream>
#include <bits/stdc++.h>
using namespace std;

int main() {
	//code
	int t;
	cin>>t;
	while(t--)
	{
	    int m, n, count=0, x, i;
	    cin>>m>>n;
	    int arr1[m], arr2[n];
	    set <int> s1;
	    for(i=0; i<m; i++)
	        cin>>arr1[i];
	    for(i=0; i<n; i++)
	        cin>>arr2[i];
	    for(i=0; i<m; i++)
	        s1.insert(arr1[i]);
	    cin>>x;
	    for(i = 0 ; i < m ; i++ )
    {
	    if( s1.find(x-arr2[i])!=s1.end() )
            count++;
    }
            
        cout<<count<<endl;
	}
	return 0;
}
