
/*SET CONCEPT*/
#include <iostream>
#include <bits/stdc++.h>
using namespace std;

int main() {
	//code
	int t;
	cin>>t;
	
	while(t--)
	{
	    string s;
	    cin>>s;
	    set <char> news;
	    int i;
	    for(i=0; i<s.length(); i++)
	    {
	        news.insert(s.at(i));
	    }
	    
	    cout<<(s.length()-news.size())<<endl;
	}
	

	return 0;
}
