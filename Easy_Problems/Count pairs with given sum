#include <iostream>
using namespace std;
	int main() {
	//code
	int t;
	cin>>t;
	while(t--)
	{
	    int n, i, j, k, count=0;
	   
	    cin>>n>>k;
	    int arr[n];
	    for(i=0; i<n; i++)
	        cin>>arr[i];
	    for(i=0; i<n-1; i++)
	    {
	        for(j=i+1; j<n; j++)
	    {
	        if(arr[i]+arr[j]==k)
	            count++;
	    }
	    }
	    
	    cout<<count<<endl;
	}
	return 0;
}
