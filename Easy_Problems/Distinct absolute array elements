/*MAP STL IS USED*/
#include <iostream>
#include <list>
#include <bits/stdc++.h>
using namespace std;

int main() {
	//code
	int t;
	cin>>t;
	while(t--)
	{
	    int n;
	    cin>>n;
	 
	    map<int,int> m;
for(int i = 0; i < n; i++)
{
int data;
cin>>data;
m[abs(data)]++;
}
cout<<m.size()<<endl;  
	    
	        
	}
	return 0;
}
